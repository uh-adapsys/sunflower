<!-- -*- mode: XML -*- -->

<launch>
	<param name="/use_sim_time" value="true" />
	<include file="$(find sf_environments)/launch/uh-rh.launch" />
	<include file="$(find sf_robot)/launch/sf1-1_sim.launch" />
	<include file="$(find person_sim)/launch/start.launch" />
	<node pkg="rviz" type="rviz" name="rviz" args="-d $(find sf_simulation)/config/webots.rviz"/>
    <node pkg="webots" type="start_webots" args="$(find webots)/worlds/robotHouseWithWallsAndResearcher.wbt --mode=realtime"
	    name="webots" output="screen" required="true" />
</launch>
