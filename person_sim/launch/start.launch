<!-- -*- mode: XML -*- -->

<launch>
	<param name="/use_sim_time" value="true" />
	<arg name="ns" value="/person" />

	<group ns="$(arg ns)">
		<param name="tf_prefix" value="$(arg ns)" />
		<include file="$(find person_sim)/launch/tf.launch">
		</include>
		<include file="$(find person_sim)/launch/start_nav.launch">
			<arg name="ns" value="$(arg ns)" />
		</include>
	</group>
</launch>
