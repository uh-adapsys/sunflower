<!-- -*- mode: XML -*- -->
<launch>
	<arg name="sunflower_ns"/>
	<arg name="version" />

	<include file="$(find sf_environments)/launch/uh-rh.launch" />

	<group ns="$(arg sunflower_ns)">
		<include file="$(find sf_robot_description)/launch/tf.launch">
			<arg name="sf_version" value="$(arg version)" />
			<arg name="ns" value="$(arg sunflower_ns)" />
		</include>
		<include file="$(find sf_navigation)/launch/start.launch">
			<arg name="ns" value="$(arg sunflower_ns)" />
		</include>
	</group>
</launch>
