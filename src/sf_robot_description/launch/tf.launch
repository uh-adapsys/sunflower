<!-- -*- mode: XML -*- -->

<launch>
	<arg name="robotName" />
	<arg name="ns" default="" />
	<param name="robot_description" command="$(find xacro)/xacro.py $(find sf_robot_description)/robots/$(arg robotName).xacro" />
	<param name="tf_prefix" value="$(arg ns)" />

	<node pkg="robot_state_publisher" type="state_publisher" name="robot_state_publisher">
		<param name="publish_frequency" type="double" value="15.0" />
	</node>
</launch>
